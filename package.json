{
  "name": "spruce-moose",
  "version": "1.0.0",
  "description": "This is the backend repository for the Spruce Moose Nursery E-Commerce site. Typescript, Express, Mongoose, Swagger, OAuth 0, and Render.",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc && npm run swagger",
    "start": "nodemon --exec ts-node src/server.ts",
    "swagger": "ts-node scripts/swagger.ts && ts-node scripts/swagger-local.ts",
    "seed": "ts-node scripts/seed.ts",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calebbarzee/spruce_moose_backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calebbarzee/spruce_moose_backend/issues"
  },
  "homepage": "https://github.com/calebbarzee/spruce_moose_backend#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.5.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "swagger-autogen": "^2.23.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openid-connect": "^2.12.0",
    "mongodb": "^5.0.0",
    "mongoose": "^6.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "typescript": "^4.9.5"
  }
}
